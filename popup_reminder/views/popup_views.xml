<openerp>
    <data>
        <template id="assets_backend" name="popup_reminder assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/popup_reminder/static/src/css/popup_reminder.css"/>
                <script type="text/javascript" src="/popup_reminder/static/src/js/popup_reminder.js"></script>
            </xpath>
        </template>
        <template id="web_menu1" name="popup_reminder assets" inherit_id="web.menu">
            <xpath expr="//ul[@class='nav navbar-nav navbar-right oe_systray']" position="after">
                <ul class="nav navbar-nav navbar-right oe_popuptray"/>
            </xpath>
        </template>
    </data>
</openerp>
