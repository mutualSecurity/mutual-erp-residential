        <!--====================================== Task form view Beginning ==================================-->
        <record model="ir.ui.view" id="project.view_task">
            <field name="name">project.task.form</field>
            <field name="model">project.task</field>
            <field name="inherit_id" ref="project.view_task_form2" />
            <field name="arch" type="xml">
               <field name="name" position="replace">
                     <field name="name" />
                </field>
                <field name="date_start" position="before">
                    <field name="first_signal_time_task" />
                </field>
                <field name="partner_id" position="after">
                      <field name="cs_number_task"/>
                      <field name="bank_code_task"/>
                      <field name="branch_code_task"/>
                      <field name="city_task"/>
                      <field name="monitoring_address_task"/>
                </field>
                <field name="sequence" position="replace">

                </field>
                <field name="date_end" position="after">
                    <field name='compute_total_time'/>
                    <!--<field name="total_time"/>
                    <button name="calculate_duration" string="Total Time" type="object" class="btn-link"  />-->
                </field>
                <field name="date_deadline" position="before">
                    <field name="complaint_reference"/>
                </field>
                <notebook>
                    <page string="Timesheet">
                          <field name="tech_name_tasks" colspan="4" nolabel="1">
                            <tree editable="top" string="Timesheets">
                                <field name="technician_name_tasks"/>
                                <field name="reason_tasks" />
                                <field name="total_time_tasks" widget="float_time"/>
                                <field name="date_tasks" />
                            </tree>
                          </field>
                    </page>
                </notebook>
            </field>
        </record>
         <!--====================================== Task form view Ending ==================================-->






        <!--====================================== Issue form view Beginning ==================================-->
         <record model="ir.ui.view" id="project_issue.project_issue">
            <field name="name">Project Issue Tracker Form</field>
            <field name="model">project.issue</field>
            <field name="inherit_id" ref="project_issue.project_issue_form_view" />
            <field name="arch" type="xml">
                <field name="name" position="replace">
                     <field name="name" />
                </field>
                <field name="categ_ids" position="attributes">
                    <attribute name="string">Other Complaints</attribute>
                </field>
                <field name="id" position="attributes">
                    <attribute name="string">Complaint ID</attribute>
                </field>
                <field name="email_from" position="replace">

                </field>
                <field name="version_id" position="replace">

                </field>
                <field name="id" position="after">
                    <field name="date_start" />
                    <field name="date_end" />
                    <field name="first_signal_time"/>
                    <!--<field name="total_time" />-->
                    <field name='compute_total_time'/>
                    <field name="status" style="background:	#6495ED !important;" />
                    <field name='complaint_log_bank'/>
                </field>
                 <field name="partner_id" position="after">
                     <field name="cs_number_issue"/>
                     <field name="bank_code_issue"/>
                     <field name="branch_code_issue"/>
                     <field name="city_issue"/>
                     <field name="monitoring_address_issue"/>
                 </field>
                <field name="user_id" position="before">
                    <field name="complaint_source"/>
                    <field name="sale_order_issue"/>
                </field>
                <field name="user_id" position="after">
                    <field name="user_id_issue"/>
                </field>
                <field name="project_id" position="after">
                    <!--<button name="calculate_duration" string="Total Time" type="object" class="btn-link"  />-->
                </field>
                <!--<field name="task_id" position="after">-->
                    <!--<br/><lable><strong>Is a self complaint?</strong></lable><field name="is_self_complaint" style="margin-left:10px;"/>-->
                <!--</field>-->
                <notebook>
                    <page string="Remarks by CMS">
                        <field name="remark_by_cms"/>
                    </page>
                </notebook>
                 <notebook>
                    <page string="Timesheet">
                          <field name="tech_name" colspan="4" nolabel="1">
                            <tree editable="top" string="Timesheets">
                                <field name="technician_name"/>
                                <field name="reason" />
                                <field name="total_time" widget="float_time"/>
                                <field name="date" />
                            </tree>
                          </field>
                    </page>
                </notebook>
                <notebook>
                    <page string="Checking Criteria">
                        <group>
                            <lable><h2>Zone Signals</h2></lable><br/>
                            <field name="Zone1"/>
                            <field name="Zone2"/>
                            <field name="Zone3"/>
                            <field name="Zone4"/>
                            <field name="Zone5"/>
                            <field name="Zone6"/>
                            <field name="Zone7"/>
                            <field name="Zone8"/>
                            <field name="Zone9"/>
                        </group>
                        <group>
                             <lable><h2>Keypad Signals</h2></lable><br/>
                             <field name="panic"/>
                             <field name="duress"/>
                             <field name="medical"/>
                             <field name="fire"/>
                        </group>
                        <lable><h2>BS Connected to</h2></lable>
                        <group colspan="4">
                            <field name="gsm"/>
                        </group>
                        <group col="6">
                             <!--<lable><h2>BS Connected to</h2></lable><br/>-->
                             <field name="gsmNumber" />
                             <field name="gsm_postpaid_prepaid" widget="radio"/>
                        </group>
                        <group col="6">
                             <field name="ptcl" />
                             <field name="ptcl_dedicated_shared" widget="radio"/>
                        </group>
                        <group>
                             <field name="response_check" widget="radio"/>
                            <field name="check_by" style="width:37%%"/>
                        </group>
                    </page>
                </notebook>
                <notebook>
                    <page string="SMS">
                        <group>
                            <field name="techContact"/>
                            <field name="sms"/>
                        </group>

                            <button name="details" string="Details" type="object" class="btn-link" />
                            <button name="smsSent" string="Sent" type="object" class="btn-link"  />

                    </page>
                </notebook>
                <xpath expr="//page[@string='Extra Info']" position="attributes">
                    <attribute name="invisible">True</attribute>
                </xpath>
                <xpath expr="//page[@string='Description']" position="attributes">
                    <attribute name="string">Remarks by Bank</attribute>
                </xpath>
            </field>
        </record>
        <!--====================================== Issue form view Ending ==================================-->

        <!--====================================== List view of Issue form for metrics Begins ==================================-->
        <record id="mutual_issue_tree" model="ir.ui.view">
              <field name="name">Project Issue-Feature Tracker Tree</field>
              <field name="model">project.issue</field>
              <field name="inherit_id" ref="project_issue.project_feature_tree_view" />
              <field name="priority" eval="20"/>
              <field name="arch" type="xml">
                  <field name="priority" position="after">
                      <field name="description"/>
                      <field name="user_id_issue"/>
                      <field name="cs_number_issue"/>
                      <field name="bank_code_issue"/>
                      <field name="branch_code_issue"/>
                      <field name="city_issue"/>
                      <field name="monitoring_address_issue"/>
                      <field name="task_id" />
                      <field name="date_start"/>
                      <field name="date_end"/>
                      <field name="compute_total_time"/>
                  </field>
                  <field name="user_id" position="after">
                      <!--<field name="technicians"/>-->
                      <field name="create_date"/>
                  </field>
                  <field name="version_id" position="replace">

                  </field>
                  <field name="description" position="attributes">
                     <attribute name="string">Remarks by Bank</attribute>
                  </field>
                  <field name="name" position="attributes">
                     <attribute name="string">Complaints</attribute>
                  </field>
                  <field name="partner_id" position="replace">

                  </field>
                  <!--<field name="user_id" position="attributes">-->
                      <!--<attribute name="string">Logged by</attribute>-->
                  <!--</field>-->
            </field>
        </record>
        <!--====================================== List view of Issue form for metrics ends ==================================-->

        <!--====================================== List view of tasks form for metrics Begins ==================================-->
        <record id="mutual_task_tree" model="ir.ui.view">
              <field name="name">project.task.tree</field>
              <field name="model">project.task</field>
              <field name="inherit_id" ref="project.view_task_tree2" />
              <field name="priority" eval="20"/>
              <field name="arch" type="xml">
                 <field name="date_end" position="after">
                      <field name="reviewer_id"/>
                      <field name="cs_number_task"/>
                      <field name="bank_code_task"/>
                      <field name="branch_code_task"/>
                      <field name="city_task"/>
                      <field name="monitoring_address_task"/>
                      <field name="compute_total_time"/>
                      <field name="date_deadline"/>
                      <field name="create_date"/>
                      <field name="priority"/>
                 </field>
                  <field name="name" position="before">
                      <field name="partner_id"/>
                  </field>
                  <field name="progress" position="replace">

                  </field>
                  <field name="date_deadline" position="replace">

                  </field>
            </field>
        </record>
         <!--==========================Search View of Issues=============================-->
        <record  model="ir.ui.view" id="project_issue.view_project_issue_filter_search">
            <field name="name">Project Issue Tracker Search</field>
            <field name="model">project.issue</field>
            <field name="inherit_id" ref="project_issue.view_project_issue_filter" />
            <field name="arch" type="xml">
                <search string="Issue Tracker Search">
                    <field name="priority"/>
                    <field name="create_date"/>
                    <field name="user_id_issue"/>
                    <field name="cs_number_issue"/>
                    <field name="bank_code_issue"/>
                    <field name="branch_code_issue"/>
                    <field name="city_issue"/>
                    <field name="monitoring_address_issue"/>
                    <group string="Custome Filter">
                        <filter name="user_id_issue" string="Not Forwarded" domain="[('user_id_issue','=',False)]"/>
                        <filter name="user_id" string="Not Assigned" domain="[('user_id','=',False)]"/>
                    </group>
                </search>
            </field>
        </record>
         <!--==========================Search View of tasks=============================-->
        <record  model="ir.ui.view" id="project.view_task_search_form_search">
            <field name="name">project.task.search.form</field>
            <field name="model">project.task</field>
            <field name="inherit_id" ref="project.view_task_search_form" />
            <field name="arch" type="xml">
                <search string="Tasks">
                    <field name="priority"/>
                    <field name="create_date"/>
                    <field name="cs_number_task"/>
                    <field name="bank_code_task"/>
                    <field name="branch_code_task"/>
                    <field name="city_task"/>
                    <field name="monitoring_address_task"/>
                </search>
            </field>
        </record>
        <!--====================================== List view of tasks form for metrics ends ==================================-->

        <!--====================================== Kanban view of issue begins ==================================-->
        <record model="ir.ui.view" id="project_issue_kanban_view">
            <field name="name">Project Issue Kanban</field>
            <field name="model">project.issue</field>
            <field name="inherit_id" ref="project_issue.project_issue_kanban_view"/>
            <field name="arch" type="xml">
                <field name="name" position="after"><br/>
                    <field name="cs_number_issue"/>
                    <span style="margin-left:5px;color:royalblue;"><field name="city_issue" /></span><br/>
                    <field name="complaint_source"/>
                </field>
            </field>
        </record>
        <!--====================================== Kanban view of issue ends ==================================-->
    </data>
</openerp>